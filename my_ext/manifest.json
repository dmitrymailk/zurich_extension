{
  "name": "Page Redder",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Turns the page red when you click the icon",
  "permissions": ["activeTab", "storage", "tabs"],
  "background": {
    "scripts": ["libs/socket.io.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["page.js"]
    }
  ],
  "browser_action": {
    "default_title": "Mental app",
    "default_popup": "index.html"
  }
}
